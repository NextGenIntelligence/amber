(import "rlvm-compile")
(import "amber-types")

(compile `object `(

(import "amber-util")
(import "amber-node")
(import "amber-compile")
(import "amber-symbol")

(fun gc_start () (
    (garbage_collect)
    (return @C_NIL)
    ))

(export fun setup_gc (std) (
    (var GC (create_module std (to_sym "GC")))
    (add_function0 GC (to_sym "start") gc_start 0)
    ))

