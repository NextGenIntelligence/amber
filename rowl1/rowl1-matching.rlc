(import "rlvm-compile")
(import "rowl1-types")

(compile `object `(

; vars: list of variable indexes
; pats: pattern matrix (given by list of list)
; exps: list of expressions
(fun compile_matching (vars pats exps) (
    (if (== vars 0) (return (car exps)))
    (var v (car vars))
    (var p (car pats))
    (var pty (node_type p))
    ))

    ))
