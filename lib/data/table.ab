module std {
    table_entry ::= expr "=>" expr { ($0, $2) }
    postfix_expr
        ::= [multiline] "Table" "{" delimited(table_entry, ",") "}"
            { `Table::from_list(!$2) }

    each(tbl@Table): tbl.entries.each
    reverse_each(tbl@Table): tbl.entries.reverse_each
}
