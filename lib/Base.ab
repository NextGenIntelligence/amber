# Copy important functions from Prim module.

head:      Prim.head
arguments: Prim.arguments
is_atom:   Prim.is_atom
at:        Prim.at
store:     Prim.store

hash(obj): {
    h: 0
    for (arg in obj)
        h = 13*h + hash(arg)
    h = 13*h + hash(head(arg))
}

hash(obj) when is_atom(obj): hash(head(obj))

id(obj): obj
