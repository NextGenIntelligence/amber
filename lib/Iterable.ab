trait Iterable {
    require iterator
}

## 
trait ForwardIterator {
    require next next?
}

trait BidirectionalIterator {
    extend ForwardIterator
    require prev prev?
}

trait RandomAccessIterator {
    extend BidirectionalIterator
    require begin end advance advance? add! sub!
}

