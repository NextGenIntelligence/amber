### Basic IO functions
IO.print(obj): IO.print(stdout, obj)

IO.read_char()   : IO.read_char(stdin)
IO.read_string() : IO.read_string(stdin)
IO.read_line()   : IO.read_line(stdin)

IO.puts(io, obj): {
    IO.print(io, obj)
    IO.print(io, "\n")
}

IO.puts(obj): IO.puts(stdout, obj)
