module std {
    expr ::= [multiline] "Array" "[" delimited(expr, ",") "]" 
                { `Array::to_array(!$2) }

    each(ary@Array): {
        i: 0
        n: ary.size
        () -> {
            if (i >= n) return undefined
            v: ary[i]
            i += 1
            v
        }
    }
}
