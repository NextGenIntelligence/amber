# separated definition
fib(n): fib(n-1) + fib(n-2)
fib(1): 1
fib(2): 1
puts(fib(20))

# guard expression
fib(n)             : fib(n-1) + fib(n-2)
fib(n) when n < 3 : 1
puts(fib(20))

# lambda abstraction
fib = 1 -> 1
    | 2 -> 1
    | n -> fib(n-1) + fib(n-2)

puts(fib(20))

# if expression
fib(n): if (n < 3) 1 else fib(n-1) + fib(n-2)
puts(fib(20))
