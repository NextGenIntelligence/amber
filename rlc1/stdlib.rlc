(var rewrite (macro (name params body) `(var @name (macro @params @body))))
(rewrite define (name params body) `(var @name (lambda @params @body)))

; aliases
(var + add)
(var - sub)
(var * mul)

(define println params (do
    (print . params)
    (print '\n')
    ))
