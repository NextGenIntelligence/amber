(var fib (lambda (n) (if (lt n 2) n (add (fib (sub n 1)) (fib (sub n 2))))))
(print `((fib 10) is @(fib 10)) '\n')

(var rewrite (macro (name params body) `(var @name (macro @params @body))))
(rewrite fun (name params body) `(var @name (lambda @params @body)))
(fun plus1 (n) (add n 1))
(print (plus1 1) '\n')
